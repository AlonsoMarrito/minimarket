generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model employeed {
  id       Int    @id @default(autoincrement())
  type     String @db.VarChar
  password String @db.VarChar
  sale     sale[]
}

model products {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar
  cost Float
}

model sale {
  id        Int       @id @default(autoincrement())
  products  String    @db.VarChar
  prices    String    @db.VarChar
  total     Float
  saler     Int
  employeed employeed @relation(fields: [saler], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
